{
  "name": "@stryberventures/stryber-react-native-ui-components-v2",
  "version": "0.0.2",
  "main": "lib/commonjs/index.js",
  "react-native": "lib/module/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "homepage": "https://matterhorn.stryber.com/native/components",
  "keywords": [
    "react-native",
    "reactjs",
    "reactnative",
    "bootstrap"
  ],
  "description": "UI library for React Native",
  "repository": {
    "type": "git",
    "url": "https://github.com/stryberventures/stryber-react-native-ui-components"
  },
  "private": false,
  "scripts": {
    "storybook-watcher": "sb-rn-watcher --config-path .ondevice",
    "update-stories": "sb-rn-get-stories --config-path .ondevice",
    "android": "react-native run-android & yarn storybook-watcher",
    "ios": "react-native run-ios & yarn storybook-watcher",
    "pod": "npx pod-install",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "tsc-compile": "tsc --noemit --project tsconfig.checkAll.json",
    "build": "webpack --config webpack.config.js && node postBuild.js",
    "postinstall": "patch-package",
    "deploy": "node deploy-to-npm.js"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
